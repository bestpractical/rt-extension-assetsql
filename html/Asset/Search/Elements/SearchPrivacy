<%args>
$Object => undef
</%args>
<%init>
my $label;
if (ref($Object) eq 'RT::User') {
    $label = $Object->id == $session{'CurrentUser'}->Id
                ? loc("My saved searches")
                : loc("[_1]'s saved searches", $Object->Format);
} else {
    $label = loc("[_1]'s saved searches", $Object->Name);
}
</%init>
<% $label %>\
